<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.jolyoulu.mybatis.mapper.SysUserMapper">

    <select id="list" resultType="top.jolyoulu.mybatis.entity.SysUser">
        SELECT * FROM sys_user
    </select>
    <select id="listById" resultType="top.jolyoulu.mybatis.entity.SysUser">
        SELECT * FROM sys_user WHERE id = #{id}
    </select>
</mapper>